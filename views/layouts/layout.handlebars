<!--error displayers -->
<div class="row">
  <div class="col l12">
    {{#if success_msg}}
    <div class="card-panel green lighten-3">{{success_msg}}</div>
    {{/if}}
    {{#if error_msg}}
    <div class="card-panel red lighten-3">{{error_msg}}</div>
    {{/if}}
    {{#if error}}
    <div class="card-panel red lighten-3">{{error}}</div>
    {{/if}}
  </div>
</div>
