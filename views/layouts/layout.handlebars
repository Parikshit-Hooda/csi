<!--members section -->
<!--footer starts.. -->
<footer class="page-footer grey darken-4">
  <div class="container">
    <div class="row">
      <div class="col l6 s12">
        <form action='/contact' method='post'>
          <div class="input-field col s12">
            <input name='emailctact' id="emailctact" type="email" class="validate" required>
            <label for="emailctact">Email Address</label>
          </div>
          <div class="input-field col s12">
            <textarea name='textareactact' id="textareactact" class="materialize-textarea" required></textarea>
            <label for="textareactact">Message</label>
          </div>
          <button type='submit' class='btn waves-effect waves-light' name='ctact-btn' id='ctact-btn'>Send</button>
        </form>
      </div>
      <div class="col l4 offset-l2 s12">
        <h5 class="white-text">Quick Links</h5>
        <ul>
          <li><a class="grey-text text-lighten-3" href="/">Home</a></li>
          <li><a class="grey-text text-lighten-3" href="/about_us">About Us</a></li>
          <li><a class="grey-text text-lighten-3 disabled" href="#!">Events</a></li>
          <li><a class="grey-text text-lighten-3" href="http://csi-dtu.blogspot.in">Blog</a></li>
        </ul>
      </div>
    </div>
  </div>
  <div class="footer-copyright">
